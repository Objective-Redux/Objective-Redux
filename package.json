{
  "name": "objective-redux",
  "productName": "Objective Redux",
  "version": "0.5.1",
  "description": "An Object-oriented, light-weight, boilerplate-free wrapper for easy development and debugging.",
  "repository": {
    "url": "git@github.com:Objective-Redux/Objective-Redux.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "homepage": "https://objective-redux.github.io/Objective-Redux/",
  "scripts": {
    "build": "tsc",
    "docs": "node scripts/generate-docs.js",
    "lint": "./node_modules/.bin/eslint \"**/{.*,*}.{js,jsx,ts,tsx}\" --config ./config/.eslintrc.js  --ignore-pattern \"!**/.*.*\"",
    "lint-fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "test": "jest --config ./config/jest.config.js"
  },
  "keywords": [
    "redux",
    "redux-wrapper",
    "react",
    "reactjs",
    "slice",
    "hooks",
    "redux-tools",
    "object-oriented",
    "redux-saga",
    "state",
    "redux-helper"
  ],
  "author": "Jason Mace (https://github.com/jmace01)",
  "license": "MIT",
  "peerDependencies": {
    "redux": "^4.0.0"
  },
  "optionalDependencies": {
    "react": "^16.8.0",
    "redux-saga": "^1.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^18.11.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^26.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "ts-jest": "^26.1.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  }
}
