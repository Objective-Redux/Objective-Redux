{
  "extends": "../tsconfig.json",
  "include": [
    "../**/.*.js",
    "../**/*.js",
    "../**/*.ts",
    "../**/*.jsx",
    "../**/*.tsx"
  ],
  "exclude": [
    "../**/node_modules/**",
    "../**/dist/**",
    "../**/coverage/**"
  ],
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "baseUrl": "./",
    "sourceMap": true
  }
}
